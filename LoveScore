let name1 = prompt("Please enter your name:");
let name2 = prompt("Please enter your crush name:");

var n = Math.random();
loveScore= n * 100;
loveScore = Math.floor(n) + 1;

if (loveScore > 70) {
   alert( 'Olá, '+ name1 + ', a compatibilidade para se relacionar com ' + name2 + ' é de ' + loveScore + '%.' )
}

if   ( loveScore > 30 && loveScore <= 70 ) {
       alert ('Your love score is ' + loveScore + '%')
   }

    
else {
    alert('Your love score is ' + loveScore + '%')
}
